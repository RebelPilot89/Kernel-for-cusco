&tlmm {
	flip_det_default: flip_det_default {
		mux {
			pins = "gpio133";
			function = "gpio";
		};
		config {
			pins = "gpio133";
			drive-strength = <2>; /* 2 mA */
			bias-no-pull;
		};
	};

	fpc1020_reset_default: fpc1020_reset_default {
		mux {
			pins = "gpio91";
			function = "gpio";
		};
		config {
			pins = "gpio91";
			output-high;
			bias-pull-up;
			drive-strength = <2>;
		};
	};

	disp_v1_en {
		disp_v1_en_default: disp_v1_en_default {
			mux {
				pins = "gpio107";
				function = "gpio";
			};

			config {
				pins = "gpio107";
				drive-strength = <2>; /* 2 mA */
				bias-no-pull;
				output-enable;
			};
		};
	};

	disp_pwr_en {
		disp_pwr_en_default: disp_pwr_en_default {
			mux {
				pins = "gpio106";
				function = "gpio";
			};

			config {
				pins = "gpio106";
				drive-strength = <2>; /* 2 mA */
				bias-no-pull;
				output-enable;
			};
		};
	};

	disp_sub_v1_en {
		disp_sub_avdd_en_default: disp_sub_avdd_en_default {
			mux {
				pins = "gpio105";
				function = "gpio";
			};

			config {
				pins = "gpio105";
				drive-strength = <2>; /* 2 mA */
				bias-no-pull;
				output-enable;
			};
		};
	};

	disp_sub_v2_en {
		disp_sub_dvdd_en_default: disp_sub_dvdd_en_default {
			mux {
				pins = "gpio100";
				function = "gpio";
			};

			config {
				pins = "gpio100";
				drive-strength = <2>; /* 2 mA */
				bias-no-pull;
				output-enable;
			};
		};
	};

	ts_sub_rst_mux {
		ts_sub_rst_active: ts-sub-rst-active {
			mux {
				pins = "gpio99";
				function = "gpio";
			};
			config {
				pins = "gpio99";
				drive-strength = <2>;
				bias-disable;
			};
		};

		ts_sub_rst_suspend: ts-sub-rst-suspend {
			mux {
				pins = "gpio99";
				function = "gpio";
			};
			config {
				pins = "gpio99";
				drive-strength = <2>;
				bias-pull-down;
				output-low;
			};
		};
	};

	ts_sub_int_mux {
		ts_sub_int_active: ts-sub-int-active {
			mux {
				pins = "gpio125";
				function = "gpio";
			};
			config {
				pins = "gpio125";
				drive-strength = <2>;
				bias-pull-up;
			};
		};

		ts_sub_int_suspend: ts-sub-int-suspend {
			mux {
				pins = "gpio125";
				function = "gpio";
			};
			config {
				pins = "gpio125";
				drive-strength = <2>;
				bias-disable;
			};
		};

		ts_sub_int_release: ts-sub-int-release {
			mux {
				pins = "gpio125";
				function = "gpio";
			};
			config {
				pins = "gpio125";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};

	max17050_main_cc {
		max17050_main_cc_default: max17050_main_cc_default{
			mux {
				pins = "gpio123";
				function = "gpio";
			};

			config {
				pins = "gpio123";
				drive-strength = <2>; /* 2 mA */
				bias-no-pull;
			};
		};
	};

	max17050_flip_cc {
		max17050_flip_cc_default: max17050_flip_cc_default{
			mux {
				pins = "gpio49";
				function = "gpio";
			};

			config {
				pins = "gpio49";
				drive-strength = <2>; /* 2 mA */
				bias-no-pull;
			};
		};
	};

        ldc2112_irq { /* TI,LDC2112 */
                ldc2112_default: default {
                        mux {
                                pins = "gpio43";
                                function = "gpio";
                        };
                        config {
                                pins = "gpio43";
                                drive-strength = <2>;
                                bias-pull-up;
                        };
                };
        };

        ldc2114_irq { /* TI,LDC2114 */
                ldc2114_default: default {
                        mux {
                                pins = "gpio3";
