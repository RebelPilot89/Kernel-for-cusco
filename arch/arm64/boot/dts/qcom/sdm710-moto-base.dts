/dts-v1/;

#include "sdm710.dtsi"

/ {
	qcom,board-id = <0 0>;
/*
 * Some nodes need labels assigned to appear in __symbols__
 */
	chosen: chosen { };
	aliases: aliases { };
	reserved_memory: reserved-memory { };
	mot_batterydata: qcom,battery-data { };
	utags: utags { };
	hw: hw { };
};

&soc {
	/delete-node/ qcom,wcd-dsp-mgr;
	/delete-node/ qcom,wcd-dsp-glink;
	/delete-node/ sound-9335;
	/delete-node/ sound-tavil;
};

&slim_aud {
	/delete-node/ tasha_codec;
	/delete-node/ tavil_codec;
};
